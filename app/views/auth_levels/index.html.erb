<h1>Listing auth_levels</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Level</th>
    <th>Allowed path</th>
    <th>Action</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @auth_levels.each do |auth_level| %>
  <tr>
    <td><%= auth_level.id %></td>
    <td><%= auth_level.level %></td>
    <td><%= auth_level.allowed_path %></td>
    <td><%= auth_level.action %></td>
    <td><%= link_to 'Show', auth_level %></td>
    <td><%= link_to 'Edit', edit_auth_level_path(auth_level) %></td>
    <td><%= link_to 'Destroy', auth_level, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Auth level', new_auth_level_path %>
